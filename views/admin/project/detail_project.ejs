<div class="card">
  <div class="card-header pb-0 p-3">
    <h6 class="card-title mb-0">
      <span><%= project.name %></span>
    </h6>
    <h6 class="mb-2 text-muted text-xs">
      <%- project.tag.join(' | ') %>
    </h6>
    <span class="badge bg-gradient-info"><%= project.categoryId.name %></span>
  </div>
  <div class="card-body p-3">
    <h6>Stacks: </h6>
    <p>
      <% project.stackId.forEach((stack) => { %>
      <span class="text-uppercase text-xs border border-2 p-2 "><%= stack.name %></span>
      <% }) %>
    </p>
    <h6 class="pt-3">Description:</h6>
    <p>
      <%- project.description %>
    </p>
    <h6>Images:</h6>
    <figure class="mt-3 row">
      <% project.imageId.forEach((img) => { %>
        <div class="col-3">
          <div class="card card-body border border-radius-lg card-plain mb-4">
            <img src="http://localhost:3000/<%= img.imageUrl %>" alt="<%= project.name %>" style="width: 100%;">
          </div>
        </div>
      <% }) %>
    </figure>
    <a href="/admin/project" class="btn bg-gradient-secondary">Back</a>
  </div>
</div>